{
    "name": "streamdeck-km-trigger",
    "version": "1.0.0",
    "description": "Stream Deck plugin to trigger Keyboard Maestro macros",
    "main": "com.wes.kmtrigger.sdPlugin/bin/plugin.js",
    "scripts": {
        "build": "tsc",
        "watch": "tsc --watch",
        "link": "streamdeck link ./com.wes.kmtrigger.sdPlugin",
        "unlink": "streamdeck unlink com.wes.kmtrigger",
        "restart": "streamdeck restart com.wes.kmtrigger",
        "stop": "streamdeck stop com.wes.kmtrigger",
        "validate": "streamdeck validate ./com.wes.kmtrigger.sdPlugin",
        "pack": "streamdeck pack com.wes.kmtrigger.sdPlugin",
        "pack-dryrun": "streamdeck pack --dry-run com.wes.kmtrigger.sdPlugin",
        "logs-sdjson-kmtrigger": "tail -F ~/Library/Logs/ElgatoStreamDeck/StreamDeck.json | grep kmtrigger --line-buffered | jq --compact-output",
        "logs-sdjson-all": "tail -F ~/Library/Logs/ElgatoStreamDeck/StreamDeck.json | grep kmtrigger --line-buffered | jq --compact-output",
        "devtools": "open -a 'Brave Browser Beta' \"devtools://devtools/bundled/inspector.html?ws=127.0.0.1:12345/$(curl -s http://127.0.0.1:12345/json/list | jq -r '.[0].id')\""
    },
    "keywords": [ "streamdeck", "keyboard-maestro", "elgato" ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/ws": "^8.5.10",
        "typescript": "^5.3.0"
    },
    "dependencies": {
        "@elgato/streamdeck": "^1.0.0"
    }
}
